{
  "name": "zynga/phpunit",
  "description": "Zynga modifications to the PHPUNIT framework.",
  "type": "library",
  "keywords": [
    "phpunit",
    "xunit",
    "testing"
  ],
  "bin": [
    "bin/phpunit",
    "bin/run-single-test.hh",
    "vendor/bin/hhvm-restart-wrapper",
    "vendor/phpunit/phpunit/phpunit-vendor",
    "vendor/phpunit/phpunit/phpunit-vendor-xdebug"
  ],
  "require": {
    "hhvm": "^3.18"
  },
  "autoload": {
    "psr-4": {
      "Zynga\\PHPUnit\\": "src/Zynga/PHPUnit/",
      "PHPUnit\\": "src/PHPUnit/",
      "SebastianBergmann\\": "src/SebastianBergmann/",
      "Symfony\\": "src/Symfony/"
    }
  }
}
