{
  "name": "zynga/phpunit",
  "description": "Zynga modifications to the PHPUNIT framework.",
  "type": "library",
  "keywords": [
    "phpunit",
    "xunit",
    "testing"
  ],
  "bin": [
    "bin/phpunit",
    "bin/run-single-test.hh",
    "vendor/bin/hhvm-restart-wrapper",
    "vendor/phpunit/phpunit/phpunit-vendor",
    "vendor/phpunit/phpunit/phpunit-vendor-xdebug"
  ],
  "require": {
    "hhvm": "^3.18",
    "zynga/phpunit-hhi": "@dev"
  },
  "repositories": [
    { "type": "vcs", "url":  "git@github.com:Zynga/zynga-hhvm-phpunit-hhi.git" }
  ],
  "autoload": {
    "psr-4": {
      "Zynga\\PHPUnit\\": "src/Zynga/PHPUnit/",
      "PHPUnit\\Interfaces\\": "src/PHPUnit/Interfaces/"
    }
  }
}
