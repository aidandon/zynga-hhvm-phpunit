  "require": {
    "hhvm": "^3.18",
    "zynga/phpunit-hhi": "@dev"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Zynga/zynga-hhvm-phpunit-hhi.git"
    }
  ]
}
